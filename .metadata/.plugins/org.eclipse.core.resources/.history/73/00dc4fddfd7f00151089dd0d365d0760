package com.mathewberry.companies;

import com.huskehhh.mysql.mysql.MySQL;

public class MySQLManager {
	
	private final CompaniesCore companies;
	private MySQL db;
	
	public MySQLManager(CompaniesCore companies) 
	{
		this.companies = companies;
	}
	
	public void setupDB()
	{
		this.db = new MySQL(this.companies, "localhost", "3306", "beacon_core", "root", "");
	}
}
