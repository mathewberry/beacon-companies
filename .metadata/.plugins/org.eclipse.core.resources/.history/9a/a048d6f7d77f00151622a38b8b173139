package com.mathewberry.companies.persistant.data;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import org.bukkit.entity.Player;

import com.avaje.ebean.validation.NotNull;

@Entity
@Table(name="mc_companies")
public class Companies 
{
	@Id
	private int id;
	@NotNull 
	private String company_name;
	@NotNull
	private String company_creator_name;
	@NotNull
	private float company_blanace;
	@NotNull
	private String company_created;
	
	public void setId(int id)
	{
		this.id = id;
	}
	
	public int getId()
	{
		return this.id;
	}
	
	public void setPlayerName(String playerName)
	{
		this.company_creator_name = playerName;
	}
	
	public String getPlayerName()
	{
		return this.company_creator_name;
	}
}
